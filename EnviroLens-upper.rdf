        <skos:topConceptOf rdf:resource="http://www.informea.org/terms"/>    <rdf:Description rdf:about="http://www.informea.org/terms/def_6437">
        <rdf:value xml:lang="en">The attempt to assess, compare and frequently justify the total price or loss represented by a certain activity or expenditure with the advantage or service it provides. (Source: GEMET/ODE) /n</rdf:value>
    </rdf:Description>
        <skos:definition rdf:resource="http://www.informea.org/terms/def_6437"/>
        <skosxl:prefLabel rdf:resource="http://www.informea.org/terms/xl_en_6437"/>
