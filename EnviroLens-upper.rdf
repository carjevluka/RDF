        <skos:topConceptOf rdf:resource="http://www.informea.org/terms"/>    <rdf:Description rdf:about="http://www.informea.org/terms/def_6007">
        <rdf:value xml:lang="en">Aviation is a general concept referring to the operation and use of aircraft including all aspects relevant in the context of environment such as aircraft noise and air pollution. (drafted) /n</rdf:value>
    </rdf:Description>
        <skos:definition rdf:resource="http://www.informea.org/terms/def_6007"/>
        <skosxl:prefLabel rdf:resource="http://www.informea.org/terms/xl_en_6007"/>
        <skos:broader rdf:resource="http://www.informea.org/terms/transport"/>
    </rdf:Description>
    <rdf:Description rdf:about="http://www.informea.org/terms/xl_en_6007">
