    </rdf:Description>
        <skos:definition rdf:resource="http://www.informea.org/terms/def_6261"/>
        <skosxl:prefLabel rdf:resource="http://www.informea.org/terms/xl_en_6261"/>
        <skos:broader rdf:resource="http://www.informea.org/terms/emission"/>
