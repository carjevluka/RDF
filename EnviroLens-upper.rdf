              <rdf:Description rdf:about="http://www.informea.org/terms/tax">
        <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
        <skos:inScheme rdf:resource="http://www.informea.org/terms"/>
        <skos:topConceptOf rdf:resource="http://www.informea.org/terms"/>    <rdf:Description rdf:about="http://www.informea.org/terms/def_6334">
        <rdf:value xml:lang="en">An amount of money demanded by a government for its support or for specific facilities or services, most frequently levied upon income, property or sales. (Source: RHW/ GEMET) /n</rdf:value>
    </rdf:Description>
        <skos:definition rdf:resource="http://www.informea.org/terms/def_6334"/>
        <skosxl:prefLabel rdf:resource="http://www.informea.org/terms/xl_en_6334"/>
        <skos:broader rdf:resource="http://www.informea.org/terms/economic-instrument"/>
    </rdf:Description>
    <rdf:Description rdf:about="http://www.informea.org/terms/xl_en_6334">
