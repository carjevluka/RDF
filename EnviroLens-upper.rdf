        <rdf:value xml:lang="en">The assignment or allotment of resources to various uses in accord with a stated goal or policy. (Source: GEMET/ODE) /n</rdf:value>
    </rdf:Description>
        <skos:definition rdf:resource="http://www.informea.org/terms/def_6050"/>
        <skosxl:prefLabel rdf:resource="http://www.informea.org/terms/xl_en_6050"/>
        <skos:broader rdf:resource="http://www.informea.org/terms/economic-instrument"/>
    </rdf:Description>
    <rdf:Description rdf:about="http://www.informea.org/terms/xl_en_6050">
