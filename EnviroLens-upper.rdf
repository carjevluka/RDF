        <rdf:value xml:lang="en">A charge fixed by law for services of public officers or for use of a privilege under control of government. (Source: GEMET/WESTS) /n</rdf:value>
    </rdf:Description>
        <skos:definition rdf:resource="http://www.informea.org/terms/def_6336"/>
        <skosxl:prefLabel rdf:resource="http://www.informea.org/terms/xl_en_6336"/>
        <skos:broader rdf:resource="http://www.informea.org/terms/economic-instrument"/>
