        <skos:definition rdf:resource="http://www.informea.org/terms/def_6317"/>
        <skosxl:prefLabel rdf:resource="http://www.informea.org/terms/xl_en_6317"/>
        <skos:broader rdf:resource="http://www.informea.org/terms/trade"/>
