        <rdf:value xml:lang="en">Waste generated by residential households and comprised of any material no longer wanted or needed. (Source: GEMET/EED) /n</rdf:value>
    </rdf:Description>
        <skos:definition rdf:resource="http://www.informea.org/terms/def_6433"/>
        <skosxl:prefLabel rdf:resource="http://www.informea.org/terms/xl_en_6433"/>
