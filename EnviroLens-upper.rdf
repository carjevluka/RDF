        <rdf:value xml:lang="en">The organisms and environment of an agricultural area considered as an ecosystem. (Source: FAO) /n</rdf:value>
    </rdf:Description>
        <skos:definition rdf:resource="http://www.informea.org/terms/def_6332"/>
        <skosxl:prefLabel rdf:resource="http://www.informea.org/terms/xl_en_6332"/>
