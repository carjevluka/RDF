        <rdf:value xml:lang="en">A gaseous, liquid, or solid fuel that contains an energy content derived from a biological source.[...] (Source: GEMET/DICCHE) /n</rdf:value>
    </rdf:Description>
        <skos:definition rdf:resource="http://www.informea.org/terms/def_6253"/>
        <skosxl:prefLabel rdf:resource="http://www.informea.org/terms/xl_en_6253"/>
        <skos:broader rdf:resource="http://www.informea.org/terms/renewable-energy"/>
